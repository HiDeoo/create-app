{
  "name": "@hideoo/create-app",
  "bin": "./cli.mjs",
  "type": "module",
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test": "vitest",
    "lint": "prettier -c . && eslint . --max-warnings=0 && tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "kolorist": "1.5.1",
    "prompts": "2.4.2",
    "tiny-glob": "0.2.9",
    "validate-npm-package-name": "4.0.0"
  },
  "devDependencies": {
    "@hideoo/eslint-config": "0.4.0",
    "@hideoo/prettier-config": "0.1.1",
    "@hideoo/tsconfig": "0.1.0",
    "@types/common-tags": "1.8.1",
    "@types/node": "16.11.46",
    "@types/prompts": "2.0.14",
    "@types/validate-npm-package-name": "4.0.0",
    "common-tags": "1.8.2",
    "eslint": "8.21.0",
    "husky": "8.0.1",
    "jest-diff": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "unbuild": "0.7.6",
    "vitest": "0.20.3"
  },
  "packageManager": "pnpm@7.8.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": "@hideoo/prettier-config",
  "lint-staged": {
    "**/*": "prettier -w -u --cache",
    "!(fixtures/**/*)*.ts": "eslint --max-warnings=0"
  }
